{
    "name": "iotc-solution-builder",
    "productName": "iotc-solution-builder",
    "version": "1.0.0",
    "description": "Azure IoT Central Solution Builder",
    "main": ".webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx ./src"
    },
    "keywords": [
        "azure",
        "iot",
        "electron",
        "react",
        "typescript"
    ],
    "author": {
        "name": "sseiber",
        "email": "scott.seiber@outlook.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "electron_forge_sample2"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpackConfigs/webpack.main.config.js",
                        "devContentSecurityPolicy": "connect-src 'self' *.cloudflare.com azureiotcentral.com *.azureiotcentral.com 'unsafe-eval'",
                        "renderer": {
                            "config": "./webpackConfigs/webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/renderer/index.html",
                                    "js": "./src/renderer/index.tsx",
                                    "name": "main_window",
                                    "preload": {
                                        "js": "./src/main/preload.js"
                                    }
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.63",
        "@electron-forge/maker-deb": "^6.0.0-beta.63",
        "@electron-forge/maker-rpm": "^6.0.0-beta.63",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
        "@electron-forge/maker-zip": "^6.0.0-beta.63",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "@vercel/webpack-asset-relocator-loader": "^1.7.0",
        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.6.0",
        "electron": "17.0.0",
        "eslint": "^8.9.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^37.9.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "fork-ts-checker-webpack-plugin": "^6.5.0",
        "node-loader": "^2.0.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "~4.5.4"
    },
    "dependencies": {
        "axios": "^0.26.0",
        "electron-log": "^4.4.6",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^8.0.1",
        "mobx": "^6.3.13",
        "mobx-react-lite": "^3.2.3",
        "query-string": "^7.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1",
        "semantic-ui-react": "^2.1.1",
        "use-async-effect": "^2.2.5"
    }
}